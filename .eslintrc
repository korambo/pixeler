{
  "parser": "@typescript-eslint/parser",
  "extends": "airbnb-typescript/base",
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "rules": {
    "@typescript-eslint/no-unused-expressions": "off",

    "import/prefer-default-export": "off",
    "import/no-extraneous-dependencies": "off",

    "no-plusplus": "off",
    "default-case": "off",
    "max-len": ["error", 120]
  }
}
